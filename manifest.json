{
    "manifest_version": 2,

    "name": "NTUST Shorten URL",
    "description": "短網址外掛",
    "version": "0.0.1",

    "browser_action": {
        "default_icon": "images/icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "https://ntust.me/",
        "tabs"
    ],
    "web_accessible_resources": [
        "images/icon.png",
        "js/jquery-2.1.4.min.js"
    ]
}