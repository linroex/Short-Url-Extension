{
  "manifest_version": 2,
  "name": "NTUST Shorten URL",
  "description": "台科大學生專屬的短網址服務，可以產生像是 https://ntust.me/aaaa 的簡短網址。",
  "version": "0.0.2",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://ntust.me/",
    "tabs"
  ],
  "web_accessible_resources": [
    "images/icon.png",
    "js/jquery-2.1.4.min.js"
  ]
}